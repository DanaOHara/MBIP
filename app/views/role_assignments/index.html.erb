<p id="notice"><%= notice %></p>

<h1>Role Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Roleid</th>
      <th>Contextid</th>
      <th>Userid</th>
      <th>Timemodified</th>
      <th>Modifierid</th>
      <th>Component</th>
      <th>Itemid</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @role_assignments.each do |role_assignment| %>
      <tr>
        <td><%= role_assignment.roleid %></td>
        <td><%= role_assignment.contextid %></td>
        <td><%= role_assignment.userid %></td>
        <td><%= role_assignment.timemodified %></td>
        <td><%= role_assignment.modifierid %></td>
        <td><%= role_assignment.component %></td>
        <td><%= role_assignment.itemid %></td>
        <td><%= role_assignment.sortorder %></td>
        <td><%= link_to 'Show', role_assignment %></td>
        <td><%= link_to 'Edit', edit_role_assignment_path(role_assignment) %></td>
        <td><%= link_to 'Destroy', role_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Role Assignment', new_role_assignment_path %>

<p id="notice"><%= notice %></p>

<h1>Forum Discussions</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Forum</th>
      <th>Name</th>
      <th>Firstpost</th>
      <th>Userid</th>
      <th>Groupid</th>
      <th>Assessed</th>
      <th>Timemodified</th>
      <th>Usermodified</th>
      <th>Timestart</th>
      <th>Timeend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forum_discussions.each do |forum_discussion| %>
      <tr>
        <td><%= forum_discussion.course %></td>
        <td><%= forum_discussion.forum %></td>
        <td><%= forum_discussion.name %></td>
        <td><%= forum_discussion.firstpost %></td>
        <td><%= forum_discussion.userid %></td>
        <td><%= forum_discussion.groupid %></td>
        <td><%= forum_discussion.assessed %></td>
        <td><%= forum_discussion.timemodified %></td>
        <td><%= forum_discussion.usermodified %></td>
        <td><%= forum_discussion.timestart %></td>
        <td><%= forum_discussion.timeend %></td>
        <td><%= link_to 'Show', forum_discussion %></td>
        <td><%= link_to 'Edit', edit_forum_discussion_path(forum_discussion) %></td>
        <td><%= link_to 'Destroy', forum_discussion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Forum Discussion', new_forum_discussion_path %>
